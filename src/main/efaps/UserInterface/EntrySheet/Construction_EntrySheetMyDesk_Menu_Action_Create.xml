<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<ui-command xmlns="http://www.efaps.org/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>89af84e5-8413-4a1d-86d9-6b23841ff04d</uuid>
    <file-application>eFapsApp-Construction</file-application>
    
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Construction_EntrySheetMyDesk_Menu_Action_Create</name>
        <access>
            <role>Construction_Admin</role>
        </access>
        

        <target>
            <form>Construction_EntrySheetForm</form>
            <execute method="create"
                program="org.efaps.esjp.construction.EntrySheet">
                <property name="UseNumberGenerator4Name">true</property>
                <property name="PositionType">Construction_EntrySheetPosition</property>
                <property name="StatusGroup">Construction_EntrySheetStatus</property>
                <property name="Status">Open</property>
            </execute>
            <validate method="validateEntrySheet"
                program="org.efaps.esjp.construction.EntrySheet" />
        </target>
        

        <property name="Target">modal</property>
        <property name="TargetCreateType">Construction_EntrySheet</property>
        <property name="TargetMode">create</property>
        <property name="WindowHeight">660</property>
        <property name="WindowWidth">1200</property>
        <property name="TargetDefaultMenu">none</property>
    </definition>
</ui-command>
