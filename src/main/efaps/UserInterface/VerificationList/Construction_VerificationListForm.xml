            <trigger program="org.efaps.esjp.projects.Project"
                method="updateField4Project"
                name="Construction_VerificationListForm.project.UI_FIELD_UPDATE"
                event="UI_FIELD_UPDATE" />
        </field>
        <field name="projectData">
