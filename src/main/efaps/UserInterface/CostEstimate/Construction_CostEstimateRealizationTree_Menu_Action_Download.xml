<ui-command xmlns="http://www.efaps.org/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>761b409c-16e0-471e-9900-4df69e7e3381</uuid>
    <file-application>eFapsApp-Construction</file-application>
    
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Construction_CostEstimateRealizationTree_Menu_Action_Download</name>
        <access>
            <role>Construction_Admin</role>
        </access>
        <!-- target definitions -->
        <target>
            <execute method="downloadCostEstimate" program="org.efaps.esjp.construction.CostEstimate">
                <property name="JasperReport">Construction_CostEstimateRealization</property>
                <property name="Mime">pdf</property>
                <property name="NoDataSource">true</property>
            </execute>
        </target>
        <property name="Target">hidden</property>
        <property name="TargetShowFile">true</property>
    </definition>
</ui-command>
