<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<ui-table xmlns="http://www.efaps.org/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>b2a991e2-6244-43b6-9aed-1196bc8804bc</uuid>
    <file-application>eFapsApp-Construction</file-application>
    
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Construction_CostEstimateTree_EntrySheetTable</name>
        <field name="type">
            <property name="Attribute">Type</property>
            <property name="Label">Sales_Document2DocumentAbstract/Type.Label</property>
            <property name="ShowTypeIcon">true</property>
            <property name="ModeSearch">READONLY</property>
        </field>
        <field name="name">
            <property name="HRef">TREE</property>
            <property name="SelectAlternateOID">linkto[ToAbstractLink].oid</property>
            <property name="Phrase">$&lt;linkto[ToAbstractLink].type.label&gt; - $&lt;linkto[ToAbstractLink].attribute[Name]&gt;</property>
            <property name="Label">Sales_DocumentAbstract/Name.Label</property>
            <property name="UIProvider">org.efaps.admin.datamodel.ui.StringUI</property>
        </field>
        <field name="description">
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="Select">linkto[ToLink].attribute[Description]</property>
            <property name="Label">Sales_DocumentAbstract/Description.Label</property>
        </field>
        <field name="rateCrossTotal">
            <property name="Select">linkto[ToAbstractLink].attribute[RateCrossTotal]</property>
            <property name="Attribute">CrossTotal</property>
            <property name="Label">Sales_DocumentAbstract/RateCrossTotal.Label</property>
            <property name="Align">right</property>
        </field>
        <field name="date">
            <property name="Select">linkto[ToAbstractLink].attribute[Date]</property>
            <property name="Label">Sales_DocumentAbstract/Date.Label</property>
            <property name="ModeSearch">READONLY</property>
        </field>
        <field name="vendorLink">
            <property name="Select">linkto[ToAbstractLink].linkto[Salesperson].attribute[Name]</property>
            <property name="Label">Sales_DocumentAbstract/Salesperson.Label</property>
            <property name="ModeSearch">READONLY</property>
        </field>
        <field name="status">
            <property name="SelectAlternateOID">linkto[ToAbstractLink].oid</property>
            <property name="Select">linkto[ToAbstractLink].attribute[StatusAbstract]</property>
            <property name="Label">Sales_DocumentAbstract/Status.Label</property>
            <property name="ModeSearch">READONLY</property>
        </field>
        <field name="buttonOpenInNewWindow">
            <icon>eFapsActionNewWindow</icon>
            <property name="SelectAlternateOID">linkto[ToAbstractLink].oid</property>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="Target">popup</property>
            <property name="SortAble">false</property>
            <property name="Width">15px</property>
        </field>
    </definition>
</ui-table>
