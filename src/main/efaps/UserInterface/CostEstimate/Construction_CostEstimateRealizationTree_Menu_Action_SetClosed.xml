<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<ui-command xmlns="http://www.efaps.org/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>d6c78e95-16d3-4155-ace3-45354ef562a9</uuid>
    <file-application>eFapsApp-Construction</file-application>
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Construction_CostEstimateRealizationTree_Menu_Action_SetClosed</name>
        <access>
            <role>Construction_Admin</role>
        </access>
        <target>
            <execute method="setStatus" program="org.efaps.esjp.admin.datamodel.StatusValue">
                <property name="Status">Closed</property>
            </execute>
            <validate program="org.efaps.esjp.construction.CostEstimate" method="validateRealization"/>
            <trigger event="UI_ACCESSCHECK" method="check4Status"
                name="Construction_CostEstimateRealizationTree_Menu_Action_SetClosed.UI_ACCESSCHECK"
                program="org.efaps.esjp.admin.access.AccessCheck4UI">
                <property name="Status">Open</property>
            </trigger>
        </target>
        <property name="TargetMode">edit</property>
        <property name="AskUser">true</property>
        <property name="Submit">true</property>
        <property name="SubmitSelectedRows">-1</property>
    </definition>
</ui-command>
